/* 
Higher-order propogate terms
  PI0 = p[3].P[2].P[1].P[0];
*/

CHIP PropI
{  
  IN P[16];
  OUT PI0,PI1,PI2,PI3;
  
  PARTS:

  And4(a=P[0],b=P[1],c=P[2],d=P[3],out=PI0);
  And4(a=P[4],b=P[5],c=P[6],d=P[7],out=PI1);
  And4(a=P[8],b=P[9],c=P[10],d=P[11],out=PI2);
  And4(a=P[12],b=P[13],c=P[14],d=P[15],out=PI3);
}